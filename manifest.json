{
  "manifest_version": 2,

  "name": "Comparateur universel",
  "description": "Créer à la volée une liste personnalisee d'éléments à comparer",
  "version": "1.3",
  "browser_action": {
    "default_popup": "popup.html"
  },
 "icons": {"48": "icon48.png"},
  "permissions": [
    "storage",
    "<all_urls>",
    "http://*/",
    "activeTab",
    "tabs"
  ]
}
